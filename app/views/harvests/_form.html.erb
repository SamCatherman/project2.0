<%= form_for @harvest do |f| %>

  <%= f.label :farm_id %>
  <%= f.select :farm_id, Farm.all.collect {|farm| [farm.name, farm.id]} %>

  <%= f.label :product_id %>
  <%= f.select :product_id, Product.all.collect {|product| [product.product_name, product.id]} %>

  <%= f.label :date %>
  <%= f.text_field :date %>

  <%= f.submit %>
<% end %>
