<%= form_for([@farm, @product]) do |f| %>

  <%= f.label :product_name %>
  <%= f.text_field :product_name %>

  <%= f.label :quality %>
  <%= f.text_field :quality %>

  <%= f.label :weight %>
  <%= f.text_field :weight %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.submit %>

<% end %>
